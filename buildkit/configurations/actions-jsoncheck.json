{
	"actions": [
		{
			"@name": "json-check",
			"@displayName": "Checking json...",
			"@action": "jsoncheck",
			"@onlyOnSwitches": [
                "build"
            ],
            "@excludeOnSwitches": [
                "quick"
            ],
            "@when": "finish",
			"@types": [ "builds/courses/course", "src/courses/course" ],
			"src": "{{outputDest}}/{{course}}/course/**",
			"validIdRegex": "((co|b|a|c){1}\\-{1}[0-9]{1,5}[a-zA-Z]{1})|((co|b|a|c){1}\\-{1}[0-9]{1,5})"
		},
		{
			"@name": "json-check",
			"@displayName": "Checking json...",
			"@action": "jsoncheck",
			"@onlyOnSwitches": [
                "build"
            ],
            "@excludeOnSwitches": [
                "quick"
            ],
            "@when": "finish",
			"@types": [ "src/course" ],
			"src": "{{outputDest}}/course/**",
			"validIdRegex": "((co|b|a|c){1}\\-{1}[0-9]{1,5}[a-zA-Z]{1})|((co|b|a|c){1}\\-{1}[0-9]{1,5})"
		}
	]
}
